              "type": "string"
            }
          ]
        },
        {
          "title": "type",
          "name": "type",
          "type": "string"
        }
      ]
    },
    {
      "name": "issue",
      "pageBundle": false,
      "template": "[[workspace]]/archetypes/issue.md",
      "fields": [
        {
          "title": "title",
          "name": "title",
          "type": "string"
        },
        {
          "title": "date",
          "name": "date",
          "type": "string"
        },
        {
          "title": "draft",
          "name": "draft",
          "type": "draft"
        },
        {
          "title": "type",
          "name": "type",
          "type": "string"
        }
      ]
    }
  ],
  "frontMatter.git.enabled": true,
  "frontMatter.content.pageFolders": [
    {
      "title": "Monday",
