          "type": "string"
        },
        {
          "title": "price",
          "name": "price",
          "type": "string"
        },
        {
          "title": "date",
          "name": "date",
          "type": "datetime"
        },
        {
          "title": "map_url",
          "name": "map_url",
          "type": "string"
        },
        {
          "title": "facebook_url",
          "name": "facebook_url",
          "type": "string"
